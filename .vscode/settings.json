{
  "files.exclude": {
    ".env": false
  },
  "files.eol": "\n",
  "i18n-ally.localesPaths": ["src/languages"],
  "i18n-ally.sourceLanguage": "es-ES",
  "i18n-ally.displayLanguage": "es-ES",
  "i18n-ally.keystyle": "nested",
  "i18n-ally.namespace": true,
  "i18n-ally.pathMatcher": "{locale}/{namespaces}.json",
  "i18n-ally.regex.usageMatch": ["[^\\w\\d]t\\(['\"`]({key})['\"`]"],
}
